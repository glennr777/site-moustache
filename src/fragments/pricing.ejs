<section id="pricing">
  <div class="container container-heading">
    <h2 class="section-heading">Our prices</h2>
    <hr class="primary">
  </div>
  <div class="container flex-row scrtop-anim">
    <% for (let i = 0; i < prices.length; i++) { %>
    <div class="service-box scrtop-hide">
      <i class="<%= prices[i].icon %>"></i>
      <h3><%= prices[i].type %></h3>
      <% if (prices[i].note) { %>
      <p class="text-muted small"><%= prices[i].note %></p>
      <% } %>
      <p class="text-muted"><%= prices[i].cost %></p>
    </div>
    <% } %>
  </div>
</section>
